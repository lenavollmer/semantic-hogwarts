<?xml version="1.0"?> <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:hwhouse="houses.rdf">
<rdfs:Class rdf:ID="Wizard">
    <rdfs:comment>Wizard Class</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
</rdfs:Class>

<rdfs:Class rdf:ID="Student">
    <rdfs:comment>Student Class</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Wizard"/>
</rdfs:Class>

<rdfs:Class rdf:ID="Teacher">
    <rdfs:comment>Teacher Class</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Wizard"/>
</rdfs:Class>

<rdf:Property rdf:ID="firstAppeared">
    <rdfs:comment>First appearance in the books</rdfs:comment>
    <rdfs:domain rdf:resource="#Wizard"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="hasFamiliar">
    <rdfs:comment>Pet of the Character</rdfs:comment>
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="isInHouse">
    <rdfs:comment>House of the character</rdfs:comment>
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:domain rdf:resource="#Teacher"/>
    <rdfs:range rdf:resource="hwhouse#HogwartsHouse"/>
</rdf:Property>

<rdf:Property rdf:ID="bloodStatus">
    <rdfs:comment>Lineage of Wizard</rdfs:comment>
    <rdfs:domain rdf:resource="#Wizard"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal"/>
</rdf:Property>

<rdf:Property rdf:ID="teaches">
    <rdfs:comment>Which classes he teaches</rdfs:comment>
    <rdfs:domain rdf:resource="#Teacher"/>
    <rdfs:range rdf:resource="https://schema.org/CourseInstance"/>
</rdf:Property>

<rdf:Property rdf:ID="isAlive">
    <rdfs:comment>Status of the character</rdfs:comment>
    <rdfs:domain rdf:resource="#Wizard"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal"/>
</rdf:Property>

</rdf:RDF>