@base <#uk.hogwarts> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rel: <http://www.perceive.net/schemas/relationship/> .
@prefix uni: <https://schema.org/CollegeOrUniversity> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix class: <https://schema.org/CourseInstance> .
@prefix wizards: <wizards.rdf> .
@prefix houses: <houses.rdf> .

<#hogwarts>
    a uni:EducationalOrganization ;
    uni:name "Hogwarts" ;
    uni:founder <#RowenaRavenclaw> ;
    uni:founder <#GodricGryffindor> ;
    # TODO other 3 founders
    # uni:founder <#rowenaravenclaw> ;
    # uni:founder <#rowenaravenclaw> ;
    # uni:founder <#rowenaravenclaw> ;
    uni:foundingDate "993" ;
    uni:foundingLocation "UK" ;
    # TODO double check format
    uni:logo "https://vignette.wikia.nocookie.net/harrypotter/images/6/61/Hogwarts-Wappen_2.png/revision/latest/top-crop/width/220/height/220?cb=20131224202326&path-prefix=de" ;
    uni:subOrganization <#Ravenclaw>;
    uni:subOrganization <#Gryffindor>;
    uni:owns <#AlbusDumbledore>;
    uni:employee <#AlbusDumbledore>;
    uni:employee <#FiliusFlitwick> ;
    uni:employee <#MinervaMcGonagall>. 

<#AlbusDumbledore>
    a <wizards#Wizard> ;
    uni:owns <#Hogwarts> ;
    wizards:isInHouse <#Gryffindor> ;
    wizards:firstAppearance "First Book";
    wizard:isAlive "No" .

# Founders
<#RowenaRavenclaw>
    a <#wizard> ;
    foaf:name "Rowena Ravenclaw" .

<#GodricGryffindor>
    a <#wizard> ;
    foaf:name "Godric Gryffindor" .

# Houses
<#Ravenclaw>
    a <houses#HogwartsHouse>
    houses:hasParentOrganization <#hogwarts> ;
    houses:slogan "" ;
    houses:founder <#RowenaRavenclaw>;
    houses:hasHeadTeacher: <#FiliusFlitwick>.

<#Hufflepuff>
    a <houses#HogwartsHouse>
    houses:hasParentOrganization <#hogwarts> ;
    houses:slogan "" .

<#Slytherin>
    a <houses#HogwartsHouse>
    houses:hasParentOrganization <#hogwarts> ;
    houses:slogan "" .

<#Gryffindor>
    a <houses#HogwartsHouse>
    houses:hasParentOrganization <#hogwarts> ;
    houses:slogan "You might belong in Gryffindor, Where dwell the brave at heart, Their daring, nerve, and chivalry Set Gryffindors apart" .
    houses:founder <#GodricGryffindor>;
    houses:hasHeadTeacher: <#MinervaMcGonagall>.

# Teachers
<#FiliusFlitwick>
    a <wizards#Teacher> ;
    uni:owns <#Ravenclaw> ;
    wizards:isInHouse <#Ravenclaw> ;
    wizards:firstAppearance "First Book";
    wizards:teaches <#HistoryOfMagic>;
    wizard:isAlive "Yes" .
    # TODO Foaf:employee does not exist.

<#MinervaMcGonagall>
    a <wizards#Teacher> ;
    uni:owns <#Gryffindor> ;
    wizards:isInHouse <#Gryffindor> ;
    wizards:firstAppearance "First Book";
    wizards:teaches <#Transfiguration>;
    foaf:knows <#AlbusDumbledore> ;
    wizard:isAlive "Yes" ;
    wizard:bloodStatus "Half-blood".
    # TODO Foaf:employee does not exist.

#Courses

<#HistoryOfMagic>
    a <#CourseInstance>
    houses:hasParentOrganization <#hogwarts> ;
    houses:slogan "" .
    class:instructor <#Flitwick> #Not sure if Schema Person is FOAF Person
    #Most Students go to most courses so we do not map them

<#Transfiguration>
    a <#CourseInstance>
    houses:hasParentOrganization <#hogwarts> ;
    class:instructor <#MinervaMcGonagall> #Not sure if Schema Person is FOAF Person
    #Most Students go to most courses so we do not map them

